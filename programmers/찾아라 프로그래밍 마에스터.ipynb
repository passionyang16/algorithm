   "source": [
    "def solution(nums):\n",
    "    possible_draws = len(nums)//2\n",
    "    nums_set = list(set(nums))\n",
    "    if len(nums_set) < possible_draws:\n",
    "        return len(nums_set)\n",
    "    else:\n",
    "        return possible_draws"
   ]
